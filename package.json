    "ejs": "~2.4.1",
    "express": "~4.13.4",
    "forever": "^0.15.3",
    "fuzzy": "^0.1.3",
    "http-auth": "^3.1.1",
    "linkify-it": "^2.0.3",
    "morgan": "~1.7.0",
    "nedb": "^1.8.0",
    "serve-favicon": "~2.3.0",
    "supervisor": "^0.12.0"
  }
}
